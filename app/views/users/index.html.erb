
<p id="notice"><%= notice %></p>

<h1>Users</h1>
<div class="well">
  <div class="item">
      <div class="row">
            <div class ="col-sm-3">
              <table>
                  <thead>
                <th>Name</th>
                <th>Email</th>
                <th>Cep</th>
                <th>Number</th>
                <th>Complement</th>
                <th>Cart</th>
                <th>Usertype</th>
                <th colspan="3"></th>
              </center></tr>
              
              
              </thead>

 

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.cep %></td>
        <td><%= user.number %></td>
        <td><%= user.complement %></td>
        <td><%= user.cart_id %></td>
        <td><%= user.usertype_id %></td>
        <td><%= link_to 'Mostrar', user %></td>
        <td><%= link_to 'Editar', edit_user_path(user) %></td>
        <td><%= link_to 'Deletar', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
        </div><!-- col -->
      </div><!-- row -->
    </div><!-- item -->
</div><!--well -->

<button><%= link_to 'Log Out', logout_path, method: :delete, data: { confirm: 'Are you sure?' } %></button>

